package task4;

import java.util.Comparator;

public class Train {
	private String pointOfRoad;	
	private int hour;
	private int minutes;
	private int numberOfTrain;
	
	public Train() {}
	
	public String getPointOfRoad() {
		return pointOfRoad;
	}

	public void setPointOfRoad(String pointOfRoad) {
		this.pointOfRoad = pointOfRoad;
	}

	public int getHour() {
		return hour;
	}

	public void setHour(int hour) {
		this.hour = hour;
	}

	public int getMinutes() {
		return minutes;
	}

	public void setMinutes(int minutes) {
		this.minutes = minutes;
	}

	public int getNumberOfTrain() {
		return numberOfTrain;
	}

	public void setNumberOfTrain(int numberOfTrain) {
		this.numberOfTrain = numberOfTrain;
	}

	public Train(String pointOfRoad, int hour, int minutes, int numberOfTrain) {
		this.hour = hour;
		this.minutes = minutes;
		
		this.pointOfRoad = pointOfRoad;
		this.numberOfTrain = numberOfTrain;
	}

	
	
	}

